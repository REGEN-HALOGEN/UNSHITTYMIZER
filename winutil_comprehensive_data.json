      },
      "Disable Web Search in Start": {
        "description": "Removes Bing web search from Start menu",
        "category": "Interface",
        "safety_level": "Safe",
        "script_type": "Registry",
        "execution_time": {
          "fast_pc": 5,
          "slow_pc": 10
        },
