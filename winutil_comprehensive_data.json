  "tweaks": {
    "essential": {
      "Disable Telemetry": {
        "description": "Disables Windows telemetry and data collection services",
        "category": "Privacy",
        "safety_level": "Safe",
        "script_type": "Registry + Services",
        "execution_time": {
          "fast_pc": 15,
          "slow_pc": 30
        },
        "commands": [
          "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\DataCollection\" /v AllowTelemetry /t REG_DWORD /d 0 /f",
          "sc config DiagTrack start= disabled",
          "sc config dmwappushservice start= disabled"
        ]
      },
      "Remove Bloatware": {
        "description": "Removes pre-installed Microsoft Store apps",
        "category": "Debloat",
        "safety_level": "Safe",
        "script_type": "PowerShell",
        "execution_time": {
          "fast_pc": 45,
          "slow_pc": 90
        },
        "commands": [
          "Get-AppxPackage *xbox* | Remove-AppxPackage",
          "Get-AppxPackage *candy* | Remove-AppxPackage",
          "Get-AppxPackage *bing* | Remove-AppxPackage"
        ]
      },
      "Disable Cortana": {
        "description": "Disables Cortana assistant",
        "category": "Privacy",
        "safety_level": "Safe",
        "script_type": "Registry",
        "execution_time": {
          "fast_pc": 10,
          "slow_pc": 20
        },
        "commands": [
          "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\Windows Search\" /v AllowCortana /t REG_DWORD /d 0 /f"
        ]
      },
      "Disable Activity History": {
        "description": "Disables Windows activity history tracking",
        "category": "Privacy",
        "safety_level": "Safe",
        "script_type": "Registry",
        "execution_time": {
          "fast_pc": 5,
          "slow_pc": 10
        },
        "commands": [
          "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\System\" /v EnableActivityFeed /t REG_DWORD /d 0 /f"
        ]
      },
      "Disable Location Tracking": {
        "description": "Turns off Windows location services",
        "category": "Privacy",
        "safety_level": "Safe",
        "script_type": "Registry",
        "execution_time": {
          "fast_pc": 5,
          "slow_pc": 10
        },
        "commands": [
          "reg add \"HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\CapabilityAccessManager\\ConsentStore\\location\" /v Value /t REG_SZ /d Deny /f"
        ]
      }
    },
