      "Gaming Mode Optimizations": {
        "description": "Optimizes CPU and GPU scheduling for gaming",
        "category": "Gaming",
        "safety_level": "Safe",
        "script_type": "Registry",
        "execution_time": {
          "fast_pc": 10,
          "slow_pc": 20
