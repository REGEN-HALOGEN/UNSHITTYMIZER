  "optimization_scripts": {
    "essential": {
      "Disable Telemetry": {
        "description": "Disables Windows telemetry and data collection",
        "risk": "Low",
        "category": "Privacy",
        "recommended_for": [
          "all"
        ],
        "execution_time": 15,
        "script": "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\DataCollection\" /v AllowTelemetry /t REG_DWORD /d 0 /f\nsc config DiagTrack start= disabled\nsc config dmwappushservice start= disabled"
      },
      "Remove Bloatware": {
        "description": "Removes pre-installed Microsoft Store apps",
        "risk": "Low",
        "category": "Debloat",
        "recommended_for": [
          "all"
        ],
        "execution_time": 60,
        "script": "Get-AppxPackage *Xbox* | Remove-AppxPackage\nGet-AppxPackage *Candy* | Remove-AppxPackage\nGet-AppxPackage *BingNews* | Remove-AppxPackage\nGet-AppxPackage *GetHelp* | Remove-AppxPackage\nGet-AppxPackage *Getstarted* | Remove-AppxPackage"
      },
      "Disable Cortana": {
        "description": "Disables Cortana assistant and search indexing",
        "risk": "Low",
        "category": "Privacy",
        "recommended_for": [
          "all"
        ],
        "execution_time": 10,
        "script": "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\Windows Search\" /v AllowCortana /t REG_DWORD /d 0 /f"
      },
      "Classic Context Menu": {
        "description": "Restores Windows 10 style right-click menu",
        "risk": "Low",
        "category": "Interface",
        "recommended_for": [
          "all"
        ],
        "execution_time": 5,
        "script": "reg add \"HKCU\\Software\\Classes\\CLSID\\{86ca1aa0-34aa-4e8b-a509-50c905bae2a2}\\InprocServer32\" /f /ve"
      }
    },
    "performance": {
      "Ultimate Performance Mode": {
        "description": "Enables Windows Ultimate Performance power plan",
        "risk": "Low",
        "category": "Performance",
        "recommended_for": [
          "Desktop",
          "High-End",
          "Enthusiast"
        ],
        "execution_time": 5,
        "script": "powercfg -duplicatescheme e9a42b02-d5df-448d-aa00-03f14749eb61"
      },
      "Gaming Optimizations": {
        "description": "Optimizes CPU and GPU scheduling for gaming",
        "risk": "Low",
        "category": "Gaming",
        "recommended_for": [
          "Mainstream",
          "High-End",
          "Enthusiast"
        ],
        "execution_time": 10,
        "script": "reg add \"HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Multimedia\\SystemProfile\\Tasks\\Games\" /v \"GPU Priority\" /t REG_DWORD /d 8 /f\nreg add \"HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Multimedia\\SystemProfile\\Tasks\\Games\" /v Priority /t REG_DWORD /d 6 /f"
      },
      "Disable Power Throttling": {
        "description": "Disables CPU power throttling for better performance",
        "risk": "Medium",
        "category": "Performance",
        "recommended_for": [
          "Desktop",
          "High-End",
          "Enthusiast"
        ],
        "execution_time": 5,
        "script": "reg add \"HKLM\\SYSTEM\\CurrentControlSet\\Control\\Power\\PowerThrottling\" /v PowerThrottlingOff /t REG_DWORD /d 1 /f"
      },
      "Network Optimization": {
        "description": "Optimizes network settings for maximum throughput",
        "risk": "Medium",
        "category": "Network",
        "recommended_for": [
          "Mainstream",
          "High-End",
          "Enthusiast"
        ],
        "execution_time": 15,
        "script": "reg add \"HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Multimedia\\SystemProfile\" /v NetworkThrottlingIndex /t REG_DWORD /d 0xffffffff /f\nnetsh int tcp set global autotuninglevel=normal"
      }
    },
    "advanced": {
      "Disable Windows Defender": {
        "description": "Disables Windows Defender real-time protection",
        "risk": "High",
        "category": "Security",
        "recommended_for": [],
        "execution_time": 20,
        "script": "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows Defender\" /v DisableAntiSpyware /t REG_DWORD /d 1 /f"
      },
      "Disable Windows Updates": {
        "description": "Completely disables automatic Windows Updates",
        "risk": "High",
        "category": "Updates",
        "recommended_for": [],
        "execution_time": 15,
        "script": "sc config wuauserv start= disabled\nreg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\WindowsUpdate\\AU\" /v NoAutoUpdate /t REG_DWORD /d 1 /f"
      }
    }
  },
